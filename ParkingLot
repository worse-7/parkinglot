package com.worst.parkinglot.model;

import lombok.Getter;

import java.util.UUID;

@Getter
public class ParkingLot {
    private String id;
    private String name;
    private Integer size;
    public ParkingLot(String name, Integer size) {
        this.id = UUID.randomUUID().toString();
        this.name = name;
        this.size = size;
    }
}
